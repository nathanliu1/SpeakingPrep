<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="370dp" android:layout_height="match_parent">

    <ImageView
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/ernest"
        android:src="@drawable/ernest"
        android:layout_marginTop="31dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/imageView2" />

    <ImageView
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/imageView2"
        android:src="@drawable/haven"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_alignTop="@+id/ernest"
        android:layout_centerHorizontal="true" />

    <ca.uwaterloo.speakingprep.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:text="Ernest Wong"
        android:id="@+id/textView"
        android:layout_below="@+id/ernest"
        android:layout_alignStart="@+id/ernest" />

    <ca.uwaterloo.speakingprep.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Haven Lau"
        android:textSize="18dp"
        android:id="@+id/textView2"
        android:layout_alignBottom="@+id/textView"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:id="@+id/nathan"
        android:src="@drawable/nathan"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView2" />

    <ca.uwaterloo.speakingprep.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nathan Liu"
        android:textSize="18dp"
        android:id="@+id/textView3"
        android:layout_marginLeft="5dp"
        android:layout_below="@+id/nathan"
        android:layout_alignStart="@+id/nathan" />

</RelativeLayout>
